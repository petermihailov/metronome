{
  "name": "metronome",
  "private": "true",
  "version": "1.14.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build --base=./",
    "build:sounds": "src/sounds/wav2mp3",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo-512x512.png",
    "preview": "vite preview",
    "update:browsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > src/utils/checkBrowser.ts",
    "update:version-file": "node -p \"require('./package.json').version\" > public/version",
    "update:minor": "npx npm-check-updates --target minor -u",
    "format": "prettier --write .",
    "lint": "eslint --fix . && stylelint --fix **/*.css",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/canvas-confetti": "^1.6.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.3.1",
    "browserslist-useragent-regexp": "^4.1.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "3.3.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-semantic-groups": "^1.2.1",
    "stylelint-use-logical": "^2.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vite-plugin-pwa": "^0.20.0"
  }
}
