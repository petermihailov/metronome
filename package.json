{
  "name": "metronome",
  "private": "true",
  "version": "1.11.2",
  "license": "UNLICENSED",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build --base=./",
    "build:sounds": "src/sounds/wav2mp3",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo-512x512.png",
    "preview": "vite preview",
    "update:browsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > src/utils/checkBrowser.ts",
    "update:version-file": "node -p \"require('./package.json').version\" > public/version",
    "update:minor": "npx npm-check-updates --target minor -u",
    "format": "prettier --write .",
    "lint": "eslint --fix . && stylelint --fix **/*.css",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "abcjs": "^6.2.2",
    "clsx": "^2.0.0",
    "immer": "^10.0.3",
    "open-props": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vite-pwa/assets-generator": "^0.0.10",
    "@vitejs/plugin-react": "^4.1.0",
    "browserslist-useragent-regexp": "^4.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-semantic-groups": "^1.2.0",
    "stylelint-use-logical": "^2.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.5",
    "workbox-window": "^7.0.0"
  }
}
